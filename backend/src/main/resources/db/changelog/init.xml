                <constraints nullable="false" primaryKey="true" primaryKeyName="species_pkey"/>
            </column>
            <column name="name" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-17" author="Bartosz Gajda">
        <addForeignKeyConstraint baseColumnNames="photos_id" baseTableName="animal_photos"
                                 constraintName="fk157pmmmijjfmfk5hrxtb293xr" onDelete="NO ACTION"
                                 referencedColumnNames="id" referencedTableName="photos"/>
    </changeSet>
