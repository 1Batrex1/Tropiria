    </changeSet>
    <changeSet id="1713128339694-6" author="Bartosz Gajda">
        <createSequence incrementBy="1" sequenceName="species_id_seq"/>
    </changeSet>
    <changeSet id="1713128339694-7" author="Bartosz Gajda">
        <createTable tableName="account">
            <column name="is_active" type="BOOLEAN"/>
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="account_pkey"/>
            </column>
            <column name="login" type="VARCHAR(255)"/>
            <column name="password" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-8" author="Bartosz Gajda">
        <createTable tableName="account_roles">
            <column name="account_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="roles_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-9" author="Bartosz Gajda">
        <createTable tableName="animal">
            <column name="sex" type="INT"/>
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="animal_pkey"/>
            </column>
            <column name="price" type="BIGINT"/>
            <column name="species_id" type="BIGINT"/>
            <column name="date_of_birth" type="VARCHAR(255)"/>
            <column name="description" type="VARCHAR(255)"/>
            <column name="name" type="VARCHAR(255)"/>
            <column name="reservation_status" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-10" author="Bartosz Gajda">
        <createTable tableName="animal_morphs">
            <column name="animal_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="morphs_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-11" author="Bartosz Gajda">
        <createTable tableName="animal_parents">
            <column name="animal_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="parents_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-12" author="Bartosz Gajda">
        <createTable tableName="animal_photos">
            <column name="animal_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
            <column name="photos_id" type="BIGINT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-13" author="Bartosz Gajda">
        <createTable tableName="morph">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="morph_pkey"/>
            </column>
            <column name="name" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-14" author="Bartosz Gajda">
        <createTable tableName="photos">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="photos_pkey"/>
            </column>
            <column name="photo_path" type="VARCHAR(255)"/>
        </createTable>
    </changeSet>
    <changeSet id="1713128339694-15" author="Bartosz Gajda">
        <createTable tableName="role">
            <column autoIncrement="true" name="id" type="BIGINT">
                <constraints nullable="false" primaryKey="true" primaryKeyName="role_pkey"/>
