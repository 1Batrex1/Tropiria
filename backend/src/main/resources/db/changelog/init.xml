        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1713128339694-1" author="Bartosz Gajda">
        <createSequence incrementBy="1" sequenceName="account_id_seq"/>
    </changeSet>
    <changeSet id="1713128339694-2" author="Bartosz Gajda">
        <createSequence incrementBy="1" sequenceName="animal_id_seq"/>
    </changeSet>
